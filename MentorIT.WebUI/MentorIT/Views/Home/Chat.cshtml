@{
    ViewBag.Title = "Chat Page";
    
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <h2>Chat Room</h2>

    <div class="main">
        <div id="loginBlock">
            Nickname:<br />
            <input id="txtUserName" type="text" />
            <input id="btnLogin" type="button" value="Enter Chat" />
        </div>
        <div id="chatBody">
            <div id="header"></div>
            <div id="inputForm">
                <input type="text" id="message" />
                <input type="button" id="sendmessage" value="Send" />
            </div>
            <div id="chatroom"></div>

            <div id="chatusers">
                <p><b>All Active Users</b></p>
            </div>
        </div>
        <input id="hdId" type="hidden" />
        <input id="username" type="hidden" />
    </div>


   
</body>
</html>